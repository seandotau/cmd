[
  {
   "cmd": "cd /Volumes/boot",
   "note": "change directory"
  },
  {
   "cmd": "touch ssh",
   "note": "create ssh file"
  },
  {
    "cmd": "touch /Volumes/boot/wpa_supplicant.conf",
    "note": "create wpa_supplicant file"
  },
  {
    "cmd": "nano /Volumes/boot/wpa_supplicant.conf",
    "note": "open wpa_supplicant file"
  },
  {
    "cmd": "country=AU\nupdate_config=1\nctrl_interface=DIR=/var/run/wpa_supplicant GROUP=netdev\nnetwork={\nssid=\"your_wifi_ssid\"\npsk=\"your_wifi_password\"\nkey_mgmt=WPA-PSK\n}",
    "note": "content of wpa_supplicant file"
  }
]
